{
  "name": "pexelsapis",
  "description": "Node interaction with www.pexels.com.",
  "version": "2023.8.29",
  "main": "./dist/esm/pexelsapis.js",
  "module": "./dist/esm/pexelsapis.js",
  "types": "./dist/esm/pexelsapis.d.ts",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Simone Gauli",
    "email": "simonegauli@gmail.com"
  },
  "exports": {
    "types": "./dist/esm/pexelsapis.d.ts",
    "import": "./dist/esm/pexelsapis.js",
    "require": "./dist/cjs/pexelsapis.js"
  },
  "scripts": {
    "generate:package.json": "tsx ./tools/generate-commonjs-package-json.ts dist/cjs/package.json",
    "dev": "tsc -w & node --watch .",
    "start": "node .",
    "lint": "eslint",
    "build": "rimraf dist && eslint && tsc && npm run build:cjs && npm run generate:package.json",
    "build:cjs": "tsc --module commonjs --moduleResolution node --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.11.22",
    "eslint": "^8.57.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.5.0"
  }
}
